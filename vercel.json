{
  "builds": [
    {
      "src": "pip install --no-cache-dir -r requirements.txt",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "."
      }
    },
    {
      "src": "index.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "(.*)",
      "destination": "index.py"
    }
  ],
  "trailingSlash": true
    }
  ],
   "version": 2,
   "routes": [
      {
         "src": "/(.*)",
         "dest": "/",
          "headers": {
              "cache-control": "ms-maxage=1000"
            }
        }
    ]
  }
{
